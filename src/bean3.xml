       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="mybean" class="com.ayang.spring5.aop.factoryBean.MyBean"></bean>
    <bean id="orders" class="com.ayang.spring5.aop.bean.Orders" init-method="initMethod" destroy-method="destoryMethod">
        <property name="name" value="手机"></property>
